{
    "manifest_version": 3,
    "name": "Remédio alerta",
    "version": "1.0",
    "description": "Essa extensão lembra a hora do remédio.",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "assets/images/icon-192x192.png",
        "48": "assets/images/icon-192x192.png",
        "128": "assets/images/icon-192x192.png"
      }
    },
    "background": {
      "service_worker": "js/background.js"
    },
    "permissions": [
    "notifications",
    "storage",
    "alarms",
    "activeTab",
    "scripting",
    "tabs"
    ],
    "icons": {
      "16": "assets/images/icon-192x192.png",
      "48": "assets/images/icon-192x192.png",
      "128": "assets/images/icon-192x192.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["js/content.js"]
      }
    ]
  }
  